for number in range(1,11,3):
    print(number,end=" ")

total=0
for number in range (1,101):
    total+=number 
print(total)



# even number sum

# Method 1
ranging= int(input())
total= 0
for i in range(0,ranging+1):
    if i%2==0:
        total+=i
        print(i,sep="+",end=" ")
    else:
        continue

print(total)

# Method 2

ranging=int(input())
total=0
for i in range(2,ranging+1,2):
    total+=ranging
print(total)
